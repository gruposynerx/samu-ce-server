{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.7",
        "geekcom/validator-docs": "^3.9",
        "guzzlehttp/guzzle": "^7.2",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/telescope": "^4.17",
        "laravel/tinker": "^2.8",
        "madnest/madzipper": "^1.4",
        "php-amqplib/php-amqplib": "^3.7",
        "pusher/pusher-php-server": "^7.2",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-permission": "^6.1",
        "spatie/laravel-webhook-client": "^3.4",
        "staudenmeir/belongs-to-through": "^2.5",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "^14.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "knuckleswtf/scribe": "^4.26",
        "laradumps/laradumps": "^3.0",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "lucascudo/laravel-pt-br-localization": "^2.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0",
        "worksome/request-factories": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "lint": "./vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
